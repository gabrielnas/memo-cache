{
  "name": "memo-cache",
  "version": "1.0.12",
  "description": "A Redis cache",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "nyc node_modules/.bin/mocha  --require ts-node/register  ./test/**/*.ts  --timeout 15000 --exit",
    "coverage": "nyc --reporter=text-lcov report  | coveralls",
    "commit": "git-cz"
  },
  "author": "",
  "license": "ISC",
  "nyc": {
    "all": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "reporter": [
      "lcov",
      "text-lcov"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.21",
    "@types/sinon": "^7.0.13",
    "@types/uuid": "^3.4.4",
    "assert-throws-async": "^3.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "sinon": "^7.3.2",
    "ts-node": "^8.0.1",
    "typescript": "^3.2.4",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "@types/ioredis": "^4.0.12",
    "@types/lodash": "^4.14.121",
    "ioredis": "^4.10.0",
    "lodash": "^4.17.11",
    "uuid": "^3.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
